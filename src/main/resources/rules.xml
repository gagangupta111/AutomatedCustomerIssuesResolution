<?xml version="1.0" encoding="UTF-8"?>
<rules>
<rule name = "R1">
	<description>DistributedMessaging Errror</description>
    <conditions>
		<condition name="condition1">
			<message>
				<token>abc</token>
				<token>def</token>
				<token>ghi</token>
				<regex>[0-9a-z]{4}-[0-9a-z]{4}-[0-9a-z]{4}-[0-9a-z]{4}</regex>
				<var>var1</var>
			</message>
		</condition>
		<condition name="condition2">
			<message>
				<token>abc</token>
				<token>def</token>
				<token>ghi</token>
				<regex>[0-9a-z]{4}-[0-9a-z]{4}-[0-9a-z]{4}-[0-9a-z]{4}</regex>
				<var>{var1}</var>
			</message>
		</condition>
	</conditions>
	<query>( condition1 AND AFTER 00:00:00:100 condition2 )
	</query>
    <actions>Solution1: R1:		1. DistributedMessaging Errror. Check if ignite is working fine.</actions>
</rule>
	<rule name = "R2">
		<description>DistributedMessaging Errror</description>
		<conditions>
			<condition name="condition1">
				<message>
					<token>abc</token>
					<token>def</token>
					<token>ghi</token>
				</message>
			</condition>
			<condition name="condition2">
				<message>
					<var>{var1}</var>
				</message>
			</condition>
		</conditions>
		<query>( condition1 AND condition2 )
		</query>
		<actions>Solution1: R1:		1. DistributedMessaging Errror. Check if ignite is working fine.</actions>
	</rule>
</rules>
